<div class="grid gap-3">
  <label for="add-supplier-dialog-company-name"> Company Name </label>
  <input
    type="text"
    id="add-supplier-dialog-company-name"
    placeholder="Enter company name"
    name="company_name"
    required
  />
</div>
<div class="grid gap-3">
  <label for="add-supplier-dialog-contact"> Contact No. </label>
  <input
    type="number"
    id="add-supplier-dialog-contact"
    placeholder="Enter contact no."
    name="contact_no"
    required
  />
</div>
